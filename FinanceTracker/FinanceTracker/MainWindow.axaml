<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="FinanceTracker.MainWindow"
        Title="FinanceTracker">

    
    <!-- <Grid Background="{DynamicResource PrimaryBackground}" ColumnDefinitions="200, *"> -->
    <!--      -->
    <!-- </Grid> -->
    <Grid Background="{DynamicResource PrimaryBackground}" ColumnDefinitions="*, 100">
        <Border Padding="20">
            <Grid RowDefinitions="*, Auto">
                <StackPanel>
                    <Image Source="{SvgImage /Assets/Images/expensely-logo.svg}" Width="320"></Image>
                </StackPanel>
            </Grid>
        </Border>
    </Grid>
</Window>
